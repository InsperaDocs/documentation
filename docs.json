{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "aspen",
  "name": "Inspera Originality API",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "api": {
    "playground": {
      "display": "interactive",
      "proxy": false
    },
    "mdx": {
      "server": "https://6x6hep4ey9.execute-api.eu-central-1.amazonaws.com/production",
      "auth": {
        "method": "bearer"
      }
    }
  },
  "fonts": {
    "family": "Space Grotesk",
    "heading": {
      "family": "Space Grotesk"
    },
    "body": {
      "family": "Space Grotesk"
    }
  },
  "icons": {
    "library": "lucide"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "API Documentation",
        "icon": "webhook",
        "groups": [
          {
            "group": "API Calls",
            "icon": "webhook",
            "pages": [
              "endpoints/authorization",
              "endpoints/createeducator",
              "development"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "file-text"
        },
        {
          "anchor": "Linkedin",
          "href": "https://mintlify.com/community",
          "icon": "linkedin"
        },
        {
          "anchor": "Blog",
          "href": "https://mintlify.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/logo1.png",
    "dark": "/logo/logo1.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hi@mintlify.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}
