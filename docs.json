{
    "$schema": "https://mintlify.com/docs.json",
    "theme": "aspen",
    "name": "Inspera Originality API",
    "colors": {
        "primary": "#FF0B55",
        "light": "#FF0B55",
        "dark": "#FF0B55"
    },
    "contextual": {
        "options": [
            "copy",
            "view",
            "chatgpt",
            "claude",
            "perplexity",
            "mcp",
            "cursor",
            "vscode"
        ]
    },
"styling": {
  "codeblocks": {
    "theme": {
      "light": "catppuccin-latte",
      "dark": "catppuccin-frappe"
    }
  }
},
    "background": {
        "color": {
            "light": "#ffffff",
            "dark": "#121212"
        }
    },
    "search": {
        "prompt": "Search endpoints..."
    },
    "api": {
        "examples": {
            "defaults": "required"
        },
        "playground": {
            "display": "interactive",
            "proxy": false
        },
        "mdx": {
            "server": [
                "https://6x6hep4ey9.execute-api.eu-central-1.amazonaws.com/production",
                "https://ja0ot1lhh6.execute-api.eu-central-1.amazonaws.com/test",
              "https://nm58vmtm0b.execute-api.eu-central-1.amazonaws.com/dev"
            ],
            "auth": {
                "method": "bearer"
            }
        }
    },
  "fonts": {
    "heading": {
        "family": "Geist Mono"
    },
    "body": {
        "family": "Space Grotesk"
    }
  },

    "icons": {
        "library": "lucide"
    },
    "favicon": "/logo3.png",
    "navigation": {
        "tabs": [
            {
                "tab": "CRUD API Endpoints",
                "groups": [
                    {
                        "group": "Environments",
                        "icon": "codesandbox",
                        "color": "#FF0B55",
                        "pages": [
                            "endpoints/environments"
                        ]
                    },
                    {
                        "group": "CRUD API Endpoints",
                        "icon": "webhook",
                        "color": "#FF0B55",
                        "pages": [
                            "endpoints/authorizationcrud",
                            "endpoints/createeducator",
                            "endpoints/updateeducator",
                            "endpoints/updatestudent",
                            "endpoints/deleteuser",
                            "endpoints/useremail",
                            "endpoints/userpagination",
                            "endpoints/userpaginationlimit",
                            "endpoints/coursecode",
                            "endpoints/coursepagelimit",
                            "endpoints/institutioninfo"
                        ]
                    }
                ]
            },
            {
                "tab": "Upload API Endpoints",
                "groups": [
                    {
                        "group": "Environments",
                        "icon": "codesandbox",
                        "pages": [
                            "endpoints/sameurls"
                        ]
                    },
                    {
                        "group": "API Endpoints",
                        "icon": "webhook",
                        "pages": [
                            "endpoints/authorization",
                            "endpoints/documentupload",
                            "endpoints/checkstatus",
                            "endpoints/reporturl"
                        ]
                    }
                ]
            }
        ]
    },
    "logo": {
        "light": "logo/logo2.png",
        "dark": "logo/logo2.png"
    },
    "navbar": {
        "primary": {
            "type": "button",
            "label": "Help Center",
            "href": "https://support.inspera.com/hc/en-us/categories/15848147005085-Inspera-Originality"
        }
    }
}
