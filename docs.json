{
    "$schema": "https://mintlify.com/docs.json",
    "theme": "aspen",
    "name": "Inspera Originality API",
    "colors": {
        "primary": "#3D74B6",
        "light": "#FF0B55",
        "dark": "#FF0B55"
    },
    "styling": {
        "codeblocks": "system"
    },
    "background": {
        "decoration": "windows",
        "color": {
            "light": "#ffffff",
            "dark": "#121212"
        }
    },
    "search": {
        "prompt": "Search endpoints..."
    },
    "api": {
        "examples": {
            "defaults": "required"
        },
        "playground": {
            "display": "interactive",
            "proxy": false
        },
        "mdx": {
            "server": [
                "https://6x6hep4ey9.execute-api.eu-central-1.amazonaws.com/production",
                "https://ja0ot1lhh6.execute-api.eu-central-1.amazonaws.com/test"
            ],
            "auth": {
                "method": "bearer"
            }
        }
    },
    "fonts": {
        "family": "Space Grotesk",
        "heading": {
            "family": "Space Grotesk"
        },
        "body": {
            "family": "Space Grotesk"
        }
    },
    "icons": {
        "library": "lucide"
    },
    "favicon": "/logo3.png",
    "navigation": {
        "groups": [
            {
                "group": "Environments",
                "icon": "codesandbox",
                "pages": [
                    "endpoints/environments"
                ]
            },
            {
                "group": "API Endpoints",
                "icon": "webhook",
                "pages": [
                    "endpoints/authorization",
                    "endpoints/documentupload",
                    "endpoints/checkstatus",
                    "endpoints/reporturl"
                ]
            },
            {
                "group": "CRUD API Endpoints",
                "icon": "webhook",
                "pages": [
                    "endpoints/authorizationcrud",
                    "endpoints/createeducator",
                    "endpoints/updateeducator",
                    "endpoints/updatestudent",
                    "endpoints/deleteuser",
                    "endpoints/useremail",
                    "endpoints/userpagination",
                    "endpoints/userpaginationlimit",
                    "endpoints/coursecode",
                    "endpoints/coursepagelimit",
                    "endpoints/institutioninfo"

                ]
            }
        ]
    },
    "logo": {
        "light": "logo/logo2.png",
        "dark": "logo/logo2.png"
    },
    "navbar": {
        "primary": {
            "type": "button",
            "label": "Help Center",
            "href": "https://support.inspera.com/hc/en-us/categories/15848147005085-Inspera-Originality"
        }
    }
}